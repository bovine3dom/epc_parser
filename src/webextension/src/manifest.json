{
    "manifest_version": 2,
    "name": "EPC inliner for Zoopla and rightmove",
    "version": "0.0.3",
    "icons": {
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.zoopla.co.uk/*",
                "*://*.rightmove.co.uk/*"
            ],
            "js": [
                "epc_inliner.user.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
    ],
    "applications": {
        "gecko": {
            "id": "epc@tridactyl.xyz",
            "strict_min_version": "50.0"
        }
    }
}
